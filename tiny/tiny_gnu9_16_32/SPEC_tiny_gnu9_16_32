#!/bin/bash
#SBATCH --job-name SPEC_tiny_gnu9_16_32
#SBATCH --partition graviton3
#SBATCH --nodes 8
#SBATCH --ntasks 16
#SBATCH --ntasks-per-node 2
#SBATCH --cpus-per-task 32
#SBATCH --time 02:00:00
#SBATCH --output SPEC_tiny_gnu9_16_32.out

cd ../../../../../lustre/SPEChpc
. ./shrc
cd $SLURM_SUBMIT_DIR

spack load gcc@9.3.0 %gcc@7.3.1
spack load openmpi@4.1.3 %gcc@9.3.0

runhpc --rebuild --define model=omp --define ppn=2 --pmodel=OMP --config=aws_g3_gnu9.cfg --ranks=16 --threads=32 --reportable --output_format=all --output_root=/home/sam/g3_spec_results/tiny/tiny_gnu9_16_32 --iterations=2 tiny
