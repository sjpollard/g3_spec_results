#!/bin/bash
#SBATCH --job-name SPEC_tiny_gnu9_32_2
#SBATCH --partition graviton3
#SBATCH --nodes 1
#SBATCH --ntasks 32
#SBATCH --ntasks-per-node 32
#SBATCH --cpus-per-task 2
#SBATCH --time 08:00:00
#SBATCH --output SPEC_tiny_gnu9_32_2.out

cd ../../../../../lustre/SPEChpc
. ./shrc
cd $SLURM_SUBMIT_DIR

module load gcc@9.3.0 %gcc@7.3.1
module load openmpi@4.1.3 %gcc@9.3.0

runhpc --rebuild --define model=omp --define ppn=32 --pmodel=OMP --config=aws_g3_gnu9.cfg --ranks=32 --threads=2 --reportable --output_format=all --output_root=/home/sam/g2_spec_results/tiny/tiny_gnu9_32_2 --iterations=2 tiny
