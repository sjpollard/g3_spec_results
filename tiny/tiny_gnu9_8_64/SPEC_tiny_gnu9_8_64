#!/bin/bash
#SBATCH --job-name SPEC_tiny_gnu9_8_64
#SBATCH --partition graviton3
#SBATCH --nodes 8
#SBATCH --ntasks 8
#SBATCH --ntasks-per-node 1
#SBATCH --cpus-per-task 64
#SBATCH --time 02:00:00
#SBATCH --output SPEC_tiny_gnu9_8_64.out

cd ../../../../../lustre/SPEChpc
. ./shrc
cd $SLURM_SUBMIT_DIR

module load gcc@9.3.0 %gcc@7.3.1
module load openmpi@4.1.3 %gcc@9.3.0

runhpc --rebuild --define model=omp --define ppn=1 --pmodel=OMP --config=aws_g3_gnu9.cfg --ranks=8 --threads=64 --reportable --output_format=all --output_root=/home/sam/g2_spec_results/tiny/tiny_gnu9_8_64 --iterations=2 tiny
