#!/bin/bash
#SBATCH --job-name SPEC_tiny_gnu7_64
#SBATCH --partition graviton3
#SBATCH --nodes 1
#SBATCH --ntasks 64
#SBATCH --time 08:00:00
#SBATCH --output SPEC_tiny_gnu7_64.out

cd ../../../../../lustre/SPEChpc
. ./shrc
cd $SLURM_SUBMIT_DIR

runhpc --rebuild --config=aws_g3_gnu7.cfg --ranks=64 --noreportable --size=test --output_format=all --output_root=/home/sam/g3_spec_results/tiny/tiny_gnu7_64 --iterations=2 tiny
