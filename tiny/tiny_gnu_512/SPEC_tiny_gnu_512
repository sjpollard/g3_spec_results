#!/bin/bash
#SBATCH --job-name SPEC_tiny_gnu_512
#SBATCH --partition graviton3
#SBATCH --nodes 8
#SBATCH --time 00:30:00
#SBATCH --output SPEC_tiny_gnu_512.out

module load openmpi/4.1.1

runhpc --rebuild --config=aws_g3_gnu_8.cfg --ranks=512 --noreportable --output_format=text --output_root=/home/sam/g3_spec_results/tiny/tiny_gnu_512 --iterations=1 tiny

